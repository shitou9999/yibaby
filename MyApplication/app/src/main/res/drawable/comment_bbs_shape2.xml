<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:height="50dp" android:width="120dp">
       <shape android:shape="rectangle" >
           <corners android:radius="1dp"/>
           <stroke android:width="1dp" android:color="@color/colortitle"/>
       </shape>
    </item>
    <item android:width="120dp" android:height="50dp">
         <shape android:shape="line">
             <corners android:radius="1dp"></corners>
             <stroke android:width="1dp" android:color="@color/colortitle"></stroke>
         </shape>
    </item>
</layer-list>